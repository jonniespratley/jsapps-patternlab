<li class="mdc-list-item {{#avatar}}mdc-list--avatar-list{{/avatar}}">
    {{#avatar}}
        <img class="mdc-list-item__graphic" src="{{img.avatar.src}}"/>    
    {{/avatar}}
    {{#icon}}
        <span class="material-icons mdc-list-item__graphic">{{ icon }}</span>
    {{/icon}}
    <span class="mdc-list-item__text">{{ title }} </span>
  </li>