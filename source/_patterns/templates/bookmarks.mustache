<style>
 body {
  background: var(--mdc-theme-background);
}

:root {
  --md-background-color: rgb(248, 249, 250);
  
  --md-loading-message-color: #6e6e6e;
  --md-toolbar-color: rgb(51, 103, 214);
  
  --mdc-theme-primary: var(--md-toolbar-color);
  --mdc-theme-secondary: #343a40;
  --mdc-theme-background: var(--md-background-color);
  --card-max-width: 960px;
  --card-padding-side: 32px;
  --folder-inactive-color: #5a5a5a;
  --highlight-color: rgba(65, 132, 243, 0.16);
  --interactive-color: var(--google-blue-500);
  --iron-icon-height: 20px;
  --iron-icon-width: 20px;
  --min-sidebar-width: 256px;
  --paper-input-container-focus-color: var(--interactive-color);
  --paper-input-container-input_-_font-size: 13px;
  --primary-text-color: var(--paper-grey-900);
  --secondary-text-color: var(--paper-grey-600);
  --splitter-width: 15px;
}
.sidebar {
    -webkit-padding-start: 3px;
    display: block;
    max-width: 40%;
    min-width: var(--min-sidebar-width);
    overflow-y: auto;
    padding-top: 8px;
    user-select: none;
    width: var(--min-sidebar-width);
}

.sidebar .container {
    -webkit-padding-start: calc(var(--node-depth, 0) * var(--padding-start-per-depth));
    cursor: pointer;
    height: 40px;
    position: relative;
}
.app .menu-close {
  display: none;
}
.app.drawer--open .menu-open {
  display: none;
}
.app.drawer--open .menu-close {
  display: block;
}

.demo-cell {
  background: white;
  border-radius: 5px;
  min-height: 200px;
}

.mdc-paper {
  background: white;
}

.flex {
  display: flex;
}

.flex__item {
  flex: 1;
}

.folder-label {
    -webkit-padding-end: 8px;
    color: var(--secondary-text-color);
    flex-grow: 1;
}


.v-centered {
    align-items: center;
    display: flex;
    flex-direction: row;
}
.u-p{
  padding: 1rem;
}
</style>
<div class="app top-app-bar__frame">
  
  <header class="mdc-top-app-bar mdc-top-app-bar--dens">
    <div class="mdc-top-app-bar__row">
    <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-start">
      <span class="mdc-top-app-bar__title">Bookmarks</span>
    </section>
    <section class="mdc-toolbar__section">
    <div class="mdc-text-field mdc-text-field--full mdc-text-field--with-leading-icon">
     
    </section>
    <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-end">
      {{> molecules-app-bar-action(icon: "more_vert", label: "Settings")}}
    </section>
    </div>
  </header>

  <div class="drawer-container-flex flex">
  <div class="mdc-top-app-bar--fixed-adjust flex__item flex">
  
  <!-- Drawer -->
    <aside class="mdc-drawer mdc-drawer--permimant sidebar">
      <div class="mdc-drawer__toolbar-spacer"></div>
      <ul class="mdc-list mdc-list--dense" aria-orientation="vertical">
        {{# listItems.ten }}
        <li class="mdc-list-item v-centered folder-label">
          <span class="folder-icon mdc-list-item__graphic">
            <i class="material-icons" aria-hidden="true">folder</i>
          </span>
          <span class="menu-label elided-text" title="{{title}}">{{title}}</span>
        </li>
        {{/ listItems.ten }}
      </ul>
    </aside>

    <!-- Content -->
   <section class="flex__item">
    <div class="">
     <section class="mdc-layout-grid ">
      <div class="mdc-layout-grid__inner">
       <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-12">

        <div class="mdc-elevation--z2 demo-cell">
        &nbsp;
        &nbsp;
        &nbsp;
        &nbsp;
        &nbsp;
        </div>


       </div>
      </div>
     </section>
    </div>
   </section>
  </div>
 </div>
</div>

 

  
<script>
document.addEventListener("DOMContentLoaded", function () {
  // let drawer = new mdc.drawer.MDCTemporaryDrawer(document.querySelector(".mdc-drawer--temporary"));
  document.querySelector("a").addEventListener("click", function () {
    drawer.open = !drawer.open;
  });
});

  var listEle = document.querySelector('.mdc-list');
  var list = new mdc.list.MDCList(listEle);
  list.singleSelection = true;
</script>