<script>
//slds-show
// slds-tabs_default__content
const tabs = document.querySelector('.slds-tabs_default__nav');

tabs.addEventListener('click', function(e){

    //remove active
    let activeEl = tabs.querySelector('.slds-is-active');
    if(activeEl){
        activeEl.classList.remove('slds-is-active');
    }
    

    //set active
    //e.target.parentNode.classList.add('slds-is-active');

    let targetTabName = e.target.getAttribute('aria-controls');
    let targetTab = document.getElementById(targetTabName);
    if(targetTab){
        console.log('change tab', e, targetTabName);
        
        targetTab.classList.toggle('slds-show slds-hide');
    }
    
    
    
})

</script>